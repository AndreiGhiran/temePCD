                                      if (rowNumber >= barcodeMatrix.length) {
                                          // We have more rows than the barcode metadata allows for, ignore them.
                                          continue;
                                      }
                                      barcodeMatrix[rowNumber][column].setValue(codeword.getValue());
                                  }
                              }
                          }
